<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetGeoJSON</servlet-name>
        <servlet-class>de.i3mainz.chronontology.servlets.GetGeoJSON</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gazetteerlookup</servlet-name>
        <servlet-class>de.i3mainz.chronontology.tools.gazetteerlookup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gazetteercompare</servlet-name>
        <servlet-class>de.i3mainz.chronontology.tools.gazetteercompare</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetGazetteerSearchResult</servlet-name>
        <servlet-class>de.i3mainz.chronontology.servlets.GetGazetteerSearchResult</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetStatus</servlet-name>
        <servlet-class>de.i3mainz.chronontology.servlets.GetStatus</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDummy</servlet-name>
        <servlet-class>de.i3mainz.chronontology.servlets.GetDummy</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetGeoJSON</servlet-name>
        <url-pattern>/GetGeoJSON</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetGazetteerSearchResult</servlet-name>
        <url-pattern>/GetGazetteerSearchResult</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gazetteerlookup</servlet-name>
        <url-pattern>/gazetteerlookup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>gazetteercompare</servlet-name>
        <url-pattern>/gazetteercompare</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetStatus</servlet-name>
        <url-pattern>/GetStatus</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDummy</servlet-name>
        <url-pattern>/GetDummy</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
			30
		</session-timeout>
    </session-config>
</web-app>
